{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015"
    ],
    "module": "commonjs",
    "removeComments": false,
    "noImplicitAny": false,
    "sourceMap": false,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules",
    "lib"
  ]
}
